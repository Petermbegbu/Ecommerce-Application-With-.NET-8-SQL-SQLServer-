@model List<Category>;

<h1>Categories</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Order</th>
        </tr>
    </thead>
    
    <tbody>
        @foreach(var row in Model){
            <tr>
                <td>@row.Name</td>
                <td>@row.DisplayOrder</td>
            </tr>
        }
    </tbody>
</table>